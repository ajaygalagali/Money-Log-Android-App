        <activity android:name=".HomeActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailsActivity" android:screenOrientation="portrait"/>
        <activity
            android:name=".plusActivity"
            android:windowSoftInputMode="adjustPan" android:screenOrientation="portrait"/>
        <activity android:name=".AddNewNameActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait">

            <!--
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

